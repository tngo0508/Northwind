@model HomeSupplierViewModel
<div class="row">
    <p>Are you sure that you want to delete this supplier?</p>
    <form method="post">
        @Html.AntiForgeryToken()
        <div><input asp-for="Supplier.SupplierId"/></div>
        <div><input asp-for="Supplier.CompanyName"/></div>
        <div><input asp-for="Supplier.Country"/></div>
        <div><input asp-for="Supplier.Phone"/></div>
        <input type="submit" value="Delete"/>
        <a href="Suppliers">Cancel</a>
    </form>
</div>